<rt-import name="Link" from="react-router" />

<tr rt-scope="this.props.player.rating as rating">
  <td>{this.props.place}</td>
  <td>
    <Link to="player/{this.props.player._id}">{this.props.player.name}</Link>
  </td>
  <td>{Math.round(this.props.player.rating.os.combined)}</td>
  <td>{Math.round(this.props.player.rating.od.combined)}</td>
  <td>{Math.round(this.props.player.rating.dyp)}</td>
  <td>{Math.round(this.props.player.rating.combined)}</td>
</tr>
